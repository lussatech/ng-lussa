{
    "ecmaFeatures": {
        "jsx": true,
        "modules": true
    },
    "parser": "babel-eslint",
    "env": {
        "browser": true,
        "node": true
    },
    "rules": {
        "strict": [2, "never"],
        "react/jsx-uses-react": 2,
        "react/jsx-uses-vars": 2
    },
    "plugins": [
        "react"
    ]
}
